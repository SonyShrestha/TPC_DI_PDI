    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var marketingnameplate;
var updatedate;

updatedate = BatchDate;

marketingnameplate='';

if ((NetWorth*1 > 1000000) || (Income*1 > 200000)) {
	marketingnameplate = 'HighValue';
}

if ((NumberChildren > 3) || (NumberCreditCards > 5)) {
	if (marketingnameplate.trim()===''){
		marketingnameplate = marketingnameplate+'Expenses';
	} 
	else{
		marketingnameplate = marketingnameplate+'+ Expenses';
	}
}

if (Age > 45){
	if (marketingnameplate.trim()===''){
		marketingnameplate = marketingnameplate+'Boomer';
	} 
	else{
		marketingnameplate = marketingnameplate+'+ Boomer';
	}
}

 if ((Income &lt; 50000) || (CreditRating &lt; 600) || (NetWorth &lt; 100000)) {
	if (marketingnameplate.trim()===''){
		marketingnameplate = marketingnameplate+'MoneyAlert';
	} 
	else{
		marketingnameplate = marketingnameplate + '+ MoneyAlert';
	}
}

 if ((NumberCars > 3) || (NumberCreditCards > 7)) {
	if (marketingnameplate.trim()===''){
		marketingnameplate = marketingnameplate+'Spender';
	} 
	else{
		marketingnameplate = marketingnameplate + '+ Spender';
	}
}

 if ((Age &lt; 25) &amp;&amp; (NetWorth > 1000000)) {
	if (marketingnameplate.trim()===''){
		marketingnameplate = marketingnameplate+'Inherited';
	} 
	else{
	marketingnameplate = marketingnameplate + '+ Inherited';
}
}</jsScript_script>
